{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": 0,
        "import/extensions": 0,
        "no-console": 0,
        "no-restricted-globals": 0,
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "max-classes-per-file": 0,
        "prefer-const": 0
    }
}